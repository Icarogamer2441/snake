# utils.sk - A utility module for Snake

def add(a: int, b: int) -> int:
    return a + b;

def subtract(a: int, b: int) -> int:
    return a - b;

def multiply(a: int, b: int) -> int:
    return a * b;

def divide(a: int, b: int) -> float:
    return a / b;

# A simple Point struct
struct Point:
    x: int;
    y: int;

# A simple Color enum
enum Color:
    RED
    GREEN
    BLUE
